<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="640.0" snapToPixel="true" visible="true" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="keymix.MainController">
  <children>
    <Label id="title" alignment="CENTER" prefWidth="612.0" text="keymix.">
      <font>
        <Font size="36.0" />
      </font>
    </Label>
    <Label id="phrase" text="get on board">
      <font>
        <Font size="20.0" />
      </font>
    </Label>
    <ImageView fitHeight="236.75" fitWidth="236.75" pickOnBounds="true" preserveRatio="true" smooth="true" VBox.vgrow="ALWAYS">
      <image>
        <Image url="@../../assets/km.png" />
      </image>
      <VBox.margin>
        <Insets bottom="10.0" top="10.0" />
      </VBox.margin>
    </ImageView>
    <Button fx:id="beginButton" alignment="CENTER" mnemonicParsing="false" onAction="#handleBeginButton" text="Begin" textAlignment="CENTER" />
  </children>
</VBox>
